{
  "name": "electron-dashboard",
  "version": "1.0.0",
  "scripts": {
    "main": "rm -rf dist && webpack --watch --config webpack.config.main.js",
    "renderer": "webpack-dev-server --hot --config webpack.config.renderer.js",
    "electron": "nodemon --watch dist/main.js --exec \"electron .\"",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,json,yml}\"",
    "lint": "eslint src/**/*.{ts,tsx}",
    "m": "yarn main",
    "r": "yarn renderer",
    "e": "yarn electron"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "4.12.21",
    "regenerator-runtime": "0.13.5",
    "utlz": "2.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/html-webpack-plugin": "3.2.3",
    "@types/node": "14.0.14",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/webpack": "4.41.18",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "electron": "9.0.5",
    "eslint": "7.4.0",
    "eslint-config-ericbiewener-typescript": "5.13.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.5",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "nodemon": "2.0.4",
    "prettier-eslint-cli": "5.0.0",
    "typescript": "3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "lint-staged": {
    "**/*.{json,yml}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx,gql}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "main": "dist/main.js",
  "license": "MIT"
}
